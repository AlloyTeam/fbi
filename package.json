{
  "name": "fbi",
  "description": "A workflow tool in command-line",
  "version": "4.0.1",
  "author": "shaw @neikvon",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fbi": "./bin/run"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "./bin/run -v",
    "help": "tsc --help",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "posttest": "tslint -p test -t stylish",
    "build": "rm -rf lib && tsc",
    "watch": "npm run build && tsc --watch",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "publish": "ts-node scripts/publish.ts"
  },
  "keywords": [
    "fbi",
    "tool",
    "workflow",
    "development",
    "command line",
    "node.js"
  ],
  "license": "MIT",
  "repository": "fbi-js/fbi",
  "homepage": "https://github.com/fbi-js/fbi",
  "bugs": "https://github.com/fbi-js/fbi/issues",
  "dependencies": {
    "chalk": "^4.1.0",
    "clean-stack": "^3.0.0",
    "commander": "6.1.0",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "ora": "^5.1.0",
    "semver": "^7.3.2",
    "tiny-glob": "^0.2.6",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.10.1",
    "@types/semver": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
