{
  "name": "fbi",
  "version": "3.0.0-beta.9",
  "description": "Node.js workflow tool.",
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.10.0"
  },
  "main": "lib/index.js",
  "bin": {
    "fbi": "./bin/fbi"
  },
  "scripts": {
    "lint": "xo",
    "test": "npm run test-utils && npm run test-core",
    "test-utils": "ava test/utils.js",
    "test-core": "ava test/core/**/*.js",
    "preversion": "npm test",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "commit": "git cz",
    "publish-beta": "npm test && npm version prerelease && && git push && git push --tags && npm publish --tag next",
    "publish-patch": "npm test && npm version patch && && git push && git push --tags && npm publish",
    "publish-minor": "npm test && npm version minor && && git push && git push --tags && npm publish",
    "publish-major": "npm test && npm version major && && git push && git push --tags && npm publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.23.0",
    "cz-conventional-changelog": "^2.1.0",
    "xo": "^0.18.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neikvon/fbi.git"
  },
  "keywords": ["fbi", "workflow", "development", "template", "task"],
  "bugs": {
    "url": "https://github.com/neikvon/fbi/issues"
  },
  "homepage": "https://github.com/neikvon/fbi#readme",
  "author": "neikvon <neikvon@icloud.com>",
  "xo": {
    "esnext": true,
    "globals": [],
    "ignores": [],
    "plugins": [],
    "semicolon": false,
    "space": 2,
    "rules": {
      "camelcase": 0
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
