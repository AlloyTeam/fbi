{
  "name": "fbi",
  "version": "2.0.9",
  "description": "Node.js workflow tool",
  "engines": {
    "node": ">=4.0",
    "npm": ">= 3.0"
  },
  "bin": {
    "fbi": "./bin/fbi"
  },
  "main": "dst/index.js",
  "scripts": {
    "lint": "eslint src/** bin/**",
    "build": "rollup -c --environment entry:cli",
    "build-index": "rollup -c --environment entry:index",
    "watch": "rollup -c -w --environment entry:cli",
    "watch-index": "rollup -c -w --environment entry:index",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neikvon/fbi.git"
  },
  "keywords": [
    "fbi",
    "workflow",
    "development",
    "template",
    "task"
  ],
  "bugs": {
    "url": "https://github.com/neikvon/fbi/issues"
  },
  "homepage": "https://github.com/neikvon/fbi#readme",
  "author": "neikvon <neikvon@icloud.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "nyc": "^8.1.0",
    "rollup": "^0.34.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-watch": "^2.5.0"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "espower",
        "transform-runtime",
        "async-to-promises",
        "array-includes"
      ]
    }
  }
}
