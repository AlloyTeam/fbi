{
  "name": "fbi",
  "version": "3.0.0-beta.19",
  "description": "Node.js workflow tool.",
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.10.0"
  },
  "main": "lib/index.js",
  "bin": {
    "fbi": "./bin/fbi"
  },
  "scripts": {
    "test": "xo && nyc ava",
    "commit": "git cz",
    "publish-beta": "npm test && npm version prerelease && git push && git push --tags && npm publish --tag next",
    "publish-patch": "npm test && npm version patch && git push && git push --tags && npm publish",
    "publish-minor": "npm test && npm version minor && git push && git push --tags && npm publish",
    "publish-major": "npm test && npm version major && git push && git push --tags && npm publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release-changelog": "git add CHANGELOG.md && git commit -m 'release CHANGELOG.md' && git push"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "0.24.0",
    "cz-conventional-changelog": "^2.1.0",
    "execa": "^0.8.0",
    "nyc": "11.4.0",
    "tempfile": "^2.0.0",
    "xo": "^0.18.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neikvon/fbi.git"
  },
  "keywords": [
    "fbi",
    "workflow",
    "development",
    "template",
    "task",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/neikvon/fbi/issues"
  },
  "homepage": "https://github.com/neikvon/fbi#readme",
  "author": "neikvon <neikvon@icloud.com>",
  "xo": {
    "esnext": true,
    "globals": [],
    "ignores": [],
    "plugins": [],
    "semicolon": false,
    "space": 2,
    "rules": {
      "camelcase": 0,
      "unicorn/no-process-exit": 0,
      "no-throw-literal": 0,
      "no-await-in-loop": 0,
      "no-warning-comments": 0,
      "max-params": [
        1,
        {
          "max": 5
        }
      ],
      "no-use-extend-native/no-use-extend-native": 0,
      "no-extend-native": 0,
      "complexity": [
        "error",
        {
          "max": 25
        }
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
